@model IEnumerable<WebApplicationIdentity03.Models.Contato>

<h2>Clientes</h2>

<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.Nome)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(m => item.Nome)</td>
            </tr>
        }
    </tbody>
</table>